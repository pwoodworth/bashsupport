      <bootclasspath refid="bashsupport.module.bootclasspath"/>
      <classpath refid="bashsupport.module.production.classpath"/>
      <src refid="bashsupport.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.bashsupport"/>
    </javac2>
    
    <copy todir="${bashsupport.output.dir}">
